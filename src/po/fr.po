#: src/mainWindow.py:502
#, python-format
msgid ""
"I couldn't find the numerical ID of the user '%(user)s'.\n"
"\n"
msgstr "Je n'ai pas réussi à trouver l'ID numérique de l'utilisateur « %(user)s ».\n\n"

#: src/mainWindow.py:504
#, python-format
msgid ""
"I couldn't find the numerical IDs of these users:\n"
"%(users)s\n"
"\n"
msgstr "Je n'ai pas réussi à trouver les ID numériques des utilisateurs :\n%(users)s\n\n"

#: src/mainWindow.py:516
#, python-format
msgid ""
"I couldn't find the numerical ID of the group '%(group)s'.\n"
"\n"
msgstr "Je n'ai pas réussi à trouver l'ID numérique du groupe « %(group)s ».\n\n"

#: src/mainWindow.py:518
